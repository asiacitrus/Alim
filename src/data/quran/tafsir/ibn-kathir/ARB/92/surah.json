[
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "1",
        "Body": "وَٱلَّيْلِ إِذَا يَغْشَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "2",
        "Body": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "3",
        "Body": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "4",
        "Body": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "5",
        "Body": "فَأَمَّا مَنْ أَعْطَىٰ وَٱتَّقَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "6",
        "Body": "وَصَدَّقَ بِٱلْحُسْنَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "7",
        "Body": "فَسَنُيَسِّرُهُۥ لِلْيُسْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "8",
        "Body": "وَأَمَّا مَنۢ بَخِلَ وَٱسْتَغْنَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "9",
        "Body": "وَكَذَّبَ بِٱلْحُسْنَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "10",
        "Body": "فَسَنُيَسِّرُهُۥ لِلْعُسْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "11",
        "Body": "وَمَا يُغْنِى عَنْهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "12",
        "Body": "إِنَّ عَلَيْنَا لَلْهُدَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "13",
        "Body": "وَإِنَّ لَنَا لَلْـَٔاخِرَةَ وَٱلْأُولَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "14",
        "Body": "فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "15",
        "Body": "لَا يَصْلَىٰهَآ إِلَّا ٱلْأَشْقَى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "16",
        "Body": "ٱلَّذِى كَذَّبَ وَتَوَلَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "17",
        "Body": "وَسَيُجَنَّبُهَا ٱلْأَتْقَى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "18",
        "Body": "ٱلَّذِى يُؤْتِى مَالَهُۥ يَتَزَكَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "19",
        "Body": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةٍ تُجْزَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "20",
        "Body": "إِلَّا ٱبْتِغَآءَ وَجْهِ رَبِّهِ ٱلْأَعْلَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "92",
        "AyahNumber": "21",
        "Body": "وَلَسَوْفَ يَرْضَىٰ"
    }
]