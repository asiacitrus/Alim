[
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "1",
        "Body": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "2",
        "Body": "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "3",
        "Body": "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "4",
        "Body": "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "5",
        "Body": "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَأَخَّرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "6",
        "Body": "يَـٰٓأَيُّهَا ٱلْإِنسَـٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "7",
        "Body": "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "8",
        "Body": "فِىٓ أَىِّ صُورَةٍ مَّا شَآءَ رَكَّبَكَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "9",
        "Body": "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "10",
        "Body": "وَإِنَّ عَلَيْكُمْ لَحَـٰفِظِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "11",
        "Body": "كِرَامًا كَـٰتِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "12",
        "Body": "يَعْلَمُونَ مَا تَفْعَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "13",
        "Body": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "14",
        "Body": "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "15",
        "Body": "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "16",
        "Body": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "17",
        "Body": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "18",
        "Body": "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "82",
        "AyahNumber": "19",
        "Body": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَيْـًٔا ۖ وَٱلْأَمْرُ يَوْمَئِذٍ لِّلَّهِ"
    }
]