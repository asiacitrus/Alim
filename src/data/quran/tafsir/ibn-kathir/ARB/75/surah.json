[
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "1",
        "Body": "لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَـٰمَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "2",
        "Body": "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "3",
        "Body": "أَيَحْسَبُ ٱلْإِنسَـٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "4",
        "Body": "بَلَىٰ قَـٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "5",
        "Body": "بَلْ يُرِيدُ ٱلْإِنسَـٰنُ لِيَفْجُرَ أَمَامَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "6",
        "Body": "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَـٰمَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "7",
        "Body": "فَإِذَا بَرِقَ ٱلْبَصَرُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "8",
        "Body": "وَخَسَفَ ٱلْقَمَرُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "9",
        "Body": "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "10",
        "Body": "يَقُولُ ٱلْإِنسَـٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "11",
        "Body": "كَلَّا لَا وَزَرَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "12",
        "Body": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "13",
        "Body": "يُنَبَّؤُا۟ ٱلْإِنسَـٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "14",
        "Body": "بَلِ ٱلْإِنسَـٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "15",
        "Body": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "16",
        "Body": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "17",
        "Body": "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "18",
        "Body": "فَإِذَا قَرَأْنَـٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "19",
        "Body": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "20",
        "Body": "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "21",
        "Body": "وَتَذَرُونَ ٱلْـَٔاخِرَةَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "22",
        "Body": "وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "23",
        "Body": "إِلَىٰ رَبِّهَا نَاظِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "24",
        "Body": "وَوُجُوهٌ يَوْمَئِذٍۭ بَاسِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "25",
        "Body": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "26",
        "Body": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "27",
        "Body": "وَقِيلَ مَنْ ۜ رَاقٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "28",
        "Body": "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "29",
        "Body": "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "30",
        "Body": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "31",
        "Body": "فَلَا صَدَّقَ وَلَا صَلَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "32",
        "Body": "وَلَـٰكِن كَذَّبَ وَتَوَلَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "33",
        "Body": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "34",
        "Body": "أَوْلَىٰ لَكَ فَأَوْلَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "35",
        "Body": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "36",
        "Body": "أَيَحْسَبُ ٱلْإِنسَـٰنُ أَن يُتْرَكَ سُدًى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "37",
        "Body": "أَلَمْ يَكُ نُطْفَةً مِّن مَّنِىٍّ يُمْنَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "38",
        "Body": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "39",
        "Body": "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "75",
        "AyahNumber": "40",
        "Body": "أَلَيْسَ ذَٰلِكَ بِقَـٰدِرٍ عَلَىٰٓ أَن يُحْـِۧىَ ٱلْمَوْتَىٰ"
    }
]