[
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "1",
        "Body": "وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "2",
        "Body": "وَٱلْيَوْمِ ٱلْمَوْعُودِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "3",
        "Body": "وَشَاهِدٍ وَمَشْهُودٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "4",
        "Body": "قُتِلَ أَصْحَـٰبُ ٱلْأُخْدُودِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "5",
        "Body": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "6",
        "Body": "إِذْ هُمْ عَلَيْهَا قُعُودٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "7",
        "Body": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "8",
        "Body": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "9",
        "Body": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "10",
        "Body": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَـٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "11",
        "Body": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ جَنَّـٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "12",
        "Body": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "13",
        "Body": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "14",
        "Body": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "15",
        "Body": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "16",
        "Body": "فَعَّالٌ لِّمَا يُرِيدُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "17",
        "Body": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "18",
        "Body": "فِرْعَوْنَ وَثَمُودَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "19",
        "Body": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "20",
        "Body": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "21",
        "Body": "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "85",
        "AyahNumber": "22",
        "Body": "فِى لَوْحٍ مَّحْفُوظٍۭ"
    }
]