[
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "1",
        "Body": "ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "2",
        "Body": "خَلَقَ ٱلْإِنسَـٰنَ مِنْ عَلَقٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "3",
        "Body": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "4",
        "Body": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "5",
        "Body": "عَلَّمَ ٱلْإِنسَـٰنَ مَا لَمْ يَعْلَمْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "6",
        "Body": "كَلَّآ إِنَّ ٱلْإِنسَـٰنَ لَيَطْغَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "7",
        "Body": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "8",
        "Body": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "9",
        "Body": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "10",
        "Body": "عَبْدًا إِذَا صَلَّىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "11",
        "Body": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "12",
        "Body": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "13",
        "Body": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "14",
        "Body": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "15",
        "Body": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "16",
        "Body": "نَاصِيَةٍ كَـٰذِبَةٍ خَاطِئَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "17",
        "Body": "فَلْيَدْعُ نَادِيَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "18",
        "Body": "سَنَدْعُ ٱلزَّبَانِيَةَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "96",
        "AyahNumber": "19",
        "Body": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩"
    }
]