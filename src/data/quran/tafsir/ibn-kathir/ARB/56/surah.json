[
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "1",
        "Body": "إِذَا وَقَعَتِ ٱلْوَاقِعَةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "2",
        "Body": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "3",
        "Body": "خَافِضَةٌ رَّافِعَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "4",
        "Body": "إِذَا رُجَّتِ ٱلْأَرْضُ رَجًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "5",
        "Body": "وَبُسَّتِ ٱلْجِبَالُ بَسًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "6",
        "Body": "فَكَانَتْ هَبَآءً مُّنۢبَثًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "7",
        "Body": "وَكُنتُمْ أَزْوَٰجًا ثَلَـٰثَةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "8",
        "Body": "فَأَصْحَـٰبُ ٱلْمَيْمَنَةِ مَآ أَصْحَـٰبُ ٱلْمَيْمَنَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "9",
        "Body": "وَأَصْحَـٰبُ ٱلْمَشْـَٔمَةِ مَآ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "10",
        "Body": "وَٱلسَّـٰبِقُونَ ٱلسَّـٰبِقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "11",
        "Body": "أُو۟لَـٰٓئِكَ ٱلْمُقَرَّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "12",
        "Body": "فِى جَنَّـٰتِ ٱلنَّعِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "13",
        "Body": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "14",
        "Body": "وَقَلِيلٌ مِّنَ ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "15",
        "Body": "عَلَىٰ سُرُرٍ مَّوْضُونَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "16",
        "Body": "مُّتَّكِـِٔينَ عَلَيْهَا مُتَقَـٰبِلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "17",
        "Body": "يَطُوفُ عَلَيْهِمْ وِلْدَٰنٌ مُّخَلَّدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "18",
        "Body": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "19",
        "Body": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "20",
        "Body": "وَفَـٰكِهَةٍ مِّمَّا يَتَخَيَّرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "21",
        "Body": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "22",
        "Body": "وَحُورٌ عِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "23",
        "Body": "كَأَمْثَـٰلِ ٱللُّؤْلُؤِ ٱلْمَكْنُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "24",
        "Body": "جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "25",
        "Body": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "26",
        "Body": "إِلَّا قِيلًا سَلَـٰمًا سَلَـٰمًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "27",
        "Body": "وَأَصْحَـٰبُ ٱلْيَمِينِ مَآ أَصْحَـٰبُ ٱلْيَمِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "28",
        "Body": "فِى سِدْرٍ مَّخْضُودٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "29",
        "Body": "وَطَلْحٍ مَّنضُودٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "30",
        "Body": "وَظِلٍّ مَّمْدُودٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "31",
        "Body": "وَمَآءٍ مَّسْكُوبٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "32",
        "Body": "وَفَـٰكِهَةٍ كَثِيرَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "33",
        "Body": "لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "34",
        "Body": "وَفُرُشٍ مَّرْفُوعَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "35",
        "Body": "إِنَّآ أَنشَأْنَـٰهُنَّ إِنشَآءً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "36",
        "Body": "فَجَعَلْنَـٰهُنَّ أَبْكَارًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "37",
        "Body": "عُرُبًا أَتْرَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "38",
        "Body": "لِّأَصْحَـٰبِ ٱلْيَمِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "39",
        "Body": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "40",
        "Body": "وَثُلَّةٌ مِّنَ ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "41",
        "Body": "وَأَصْحَـٰبُ ٱلشِّمَالِ مَآ أَصْحَـٰبُ ٱلشِّمَالِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "42",
        "Body": "فِى سَمُومٍ وَحَمِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "43",
        "Body": "وَظِلٍّ مِّن يَحْمُومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "44",
        "Body": "لَّا بَارِدٍ وَلَا كَرِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "45",
        "Body": "إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُتْرَفِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "46",
        "Body": "وَكَانُوا۟ يُصِرُّونَ عَلَى ٱلْحِنثِ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "47",
        "Body": "وَكَانُوا۟ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "48",
        "Body": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "49",
        "Body": "قُلْ إِنَّ ٱلْأَوَّلِينَ وَٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "50",
        "Body": "لَمَجْمُوعُونَ إِلَىٰ مِيقَـٰتِ يَوْمٍ مَّعْلُومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "51",
        "Body": "ثُمَّ إِنَّكُمْ أَيُّهَا ٱلضَّآلُّونَ ٱلْمُكَذِّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "52",
        "Body": "لَـَٔاكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "53",
        "Body": "فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "54",
        "Body": "فَشَـٰرِبُونَ عَلَيْهِ مِنَ ٱلْحَمِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "55",
        "Body": "فَشَـٰرِبُونَ شُرْبَ ٱلْهِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "56",
        "Body": "هَـٰذَا نُزُلُهُمْ يَوْمَ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "57",
        "Body": "نَحْنُ خَلَقْنَـٰكُمْ فَلَوْلَا تُصَدِّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "58",
        "Body": "أَفَرَءَيْتُم مَّا تُمْنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "59",
        "Body": "ءَأَنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ ٱلْخَـٰلِقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "60",
        "Body": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ ٱلْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "61",
        "Body": "عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَـٰلَكُمْ وَنُنشِئَكُمْ فِى مَا لَا تَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "62",
        "Body": "وَلَقَدْ عَلِمْتُمُ ٱلنَّشْأَةَ ٱلْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "63",
        "Body": "أَفَرَءَيْتُم مَّا تَحْرُثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "64",
        "Body": "ءَأَنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ ٱلزَّٰرِعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "65",
        "Body": "لَوْ نَشَآءُ لَجَعَلْنَـٰهُ حُطَـٰمًا فَظَلْتُمْ تَفَكَّهُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "66",
        "Body": "إِنَّا لَمُغْرَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "67",
        "Body": "بَلْ نَحْنُ مَحْرُومُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "68",
        "Body": "أَفَرَءَيْتُمُ ٱلْمَآءَ ٱلَّذِى تَشْرَبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "69",
        "Body": "ءَأَنتُمْ أَنزَلْتُمُوهُ مِنَ ٱلْمُزْنِ أَمْ نَحْنُ ٱلْمُنزِلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "70",
        "Body": "لَوْ نَشَآءُ جَعَلْنَـٰهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "71",
        "Body": "أَفَرَءَيْتُمُ ٱلنَّارَ ٱلَّتِى تُورُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "72",
        "Body": "ءَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ ٱلْمُنشِـُٔونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "73",
        "Body": "نَحْنُ جَعَلْنَـٰهَا تَذْكِرَةً وَمَتَـٰعًا لِّلْمُقْوِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "74",
        "Body": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "75",
        "Body": "فَلَآ أُقْسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "76",
        "Body": "وَإِنَّهُۥ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "77",
        "Body": "إِنَّهُۥ لَقُرْءَانٌ كَرِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "78",
        "Body": "فِى كِتَـٰبٍ مَّكْنُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "79",
        "Body": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلْمُطَهَّرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "80",
        "Body": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "81",
        "Body": "أَفَبِهَـٰذَا ٱلْحَدِيثِ أَنتُم مُّدْهِنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "82",
        "Body": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "83",
        "Body": "فَلَوْلَآ إِذَا بَلَغَتِ ٱلْحُلْقُومَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "84",
        "Body": "وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "85",
        "Body": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَـٰكِن لَّا تُبْصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "86",
        "Body": "فَلَوْلَآ إِن كُنتُمْ غَيْرَ مَدِينِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "56",
        "AyahNumber": "87",
        "Body": "تَرْجِعُونَهَآ إِن كُنتُمْ صَـٰدِقِينَ"
