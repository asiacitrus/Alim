[
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "1",
        "Body": "طسٓمٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "2",
        "Body": "تِلْكَ ءَايَـٰتُ ٱلْكِتَـٰبِ ٱلْمُبِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "3",
        "Body": "لَعَلَّكَ بَـٰخِعٌ نَّفْسَكَ أَلَّا يَكُونُوا۟ مُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "4",
        "Body": "إِن نَّشَأْ نُنَزِّلْ عَلَيْهِم مِّنَ ٱلسَّمَآءِ ءَايَةً فَظَلَّتْ أَعْنَـٰقُهُمْ لَهَا خَـٰضِعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "5",
        "Body": "وَمَا يَأْتِيهِم مِّن ذِكْرٍ مِّنَ ٱلرَّحْمَـٰنِ مُحْدَثٍ إِلَّا كَانُوا۟ عَنْهُ مُعْرِضِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "6",
        "Body": "فَقَدْ كَذَّبُوا۟ فَسَيَأْتِيهِمْ أَنۢبَـٰٓؤُا۟ مَا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "7",
        "Body": "أَوَلَمْ يَرَوْا۟ إِلَى ٱلْأَرْضِ كَمْ أَنۢبَتْنَا فِيهَا مِن كُلِّ زَوْجٍ كَرِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "8",
        "Body": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "9",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "10",
        "Body": "وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰٓ أَنِ ٱئْتِ ٱلْقَوْمَ ٱلظَّـٰلِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "11",
        "Body": "قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "12",
        "Body": "قَالَ رَبِّ إِنِّىٓ أَخَافُ أَن يُكَذِّبُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "13",
        "Body": "وَيَضِيقُ صَدْرِى وَلَا يَنطَلِقُ لِسَانِى فَأَرْسِلْ إِلَىٰ هَـٰرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "14",
        "Body": "وَلَهُمْ عَلَىَّ ذَنۢبٌ فَأَخَافُ أَن يَقْتُلُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "15",
        "Body": "قَالَ كَلَّا ۖ فَٱذْهَبَا بِـَٔايَـٰتِنَآ ۖ إِنَّا مَعَكُم مُّسْتَمِعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "16",
        "Body": "فَأْتِيَا فِرْعَوْنَ فَقُولَآ إِنَّا رَسُولُ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "17",
        "Body": "أَنْ أَرْسِلْ مَعَنَا بَنِىٓ إِسْرَٰٓءِيلَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "18",
        "Body": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "19",
        "Body": "وَفَعَلْتَ فَعْلَتَكَ ٱلَّتِى فَعَلْتَ وَأَنتَ مِنَ ٱلْكَـٰفِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "20",
        "Body": "قَالَ فَعَلْتُهَآ إِذًا وَأَنَا۠ مِنَ ٱلضَّآلِّينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "21",
        "Body": "فَفَرَرْتُ مِنكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِى رَبِّى حُكْمًا وَجَعَلَنِى مِنَ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "22",
        "Body": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَىَّ أَنْ عَبَّدتَّ بَنِىٓ إِسْرَٰٓءِيلَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "23",
        "Body": "قَالَ فِرْعَوْنُ وَمَا رَبُّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "24",
        "Body": "قَالَ رَبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُم مُّوقِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "25",
        "Body": "قَالَ لِمَنْ حَوْلَهُۥٓ أَلَا تَسْتَمِعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "26",
        "Body": "قَالَ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "27",
        "Body": "قَالَ إِنَّ رَسُولَكُمُ ٱلَّذِىٓ أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "28",
        "Body": "قَالَ رَبُّ ٱلْمَشْرِقِ وَٱلْمَغْرِبِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُمْ تَعْقِلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "29",
        "Body": "قَالَ لَئِنِ ٱتَّخَذْتَ إِلَـٰهًا غَيْرِى لَأَجْعَلَنَّكَ مِنَ ٱلْمَسْجُونِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "30",
        "Body": "قَالَ أَوَلَوْ جِئْتُكَ بِشَىْءٍ مُّبِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "31",
        "Body": "قَالَ فَأْتِ بِهِۦٓ إِن كُنتَ مِنَ ٱلصَّـٰدِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "32",
        "Body": "فَأَلْقَىٰ عَصَاهُ فَإِذَا هِىَ ثُعْبَانٌ مُّبِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "33",
        "Body": "وَنَزَعَ يَدَهُۥ فَإِذَا هِىَ بَيْضَآءُ لِلنَّـٰظِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "34",
        "Body": "قَالَ لِلْمَلَإِ حَوْلَهُۥٓ إِنَّ هَـٰذَا لَسَـٰحِرٌ عَلِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "35",
        "Body": "يُرِيدُ أَن يُخْرِجَكُم مِّنْ أَرْضِكُم بِسِحْرِهِۦ فَمَاذَا تَأْمُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "36",
        "Body": "قَالُوٓا۟ أَرْجِهْ وَأَخَاهُ وَٱبْعَثْ فِى ٱلْمَدَآئِنِ حَـٰشِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "37",
        "Body": "يَأْتُوكَ بِكُلِّ سَحَّارٍ عَلِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "38",
        "Body": "فَجُمِعَ ٱلسَّحَرَةُ لِمِيقَـٰتِ يَوْمٍ مَّعْلُومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "39",
        "Body": "وَقِيلَ لِلنَّاسِ هَلْ أَنتُم مُّجْتَمِعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "40",
        "Body": "لَعَلَّنَا نَتَّبِعُ ٱلسَّحَرَةَ إِن كَانُوا۟ هُمُ ٱلْغَـٰلِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "41",
        "Body": "فَلَمَّا جَآءَ ٱلسَّحَرَةُ قَالُوا۟ لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْرًا إِن كُنَّا نَحْنُ ٱلْغَـٰلِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "42",
        "Body": "قَالَ نَعَمْ وَإِنَّكُمْ إِذًا لَّمِنَ ٱلْمُقَرَّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "43",
        "Body": "قَالَ لَهُم مُّوسَىٰٓ أَلْقُوا۟ مَآ أَنتُم مُّلْقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "44",
        "Body": "فَأَلْقَوْا۟ حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُوا۟ بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ ٱلْغَـٰلِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "45",
        "Body": "فَأَلْقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِىَ تَلْقَفُ مَا يَأْفِكُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "46",
        "Body": "فَأُلْقِىَ ٱلسَّحَرَةُ سَـٰجِدِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "47",
        "Body": "قَالُوٓا۟ ءَامَنَّا بِرَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "48",
        "Body": "رَبِّ مُوسَىٰ وَهَـٰرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "49",
        "Body": "قَالَ ءَامَنتُمْ لَهُۥ قَبْلَ أَنْ ءَاذَنَ لَكُمْ ۖ إِنَّهُۥ لَكَبِيرُكُمُ ٱلَّذِى عَلَّمَكُمُ ٱلسِّحْرَ فَلَسَوْفَ تَعْلَمُونَ ۚ لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَـٰفٍ وَلَأُصَلِّبَنَّكُمْ أَجْمَعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "50",
        "Body": "قَالُوا۟ لَا ضَيْرَ ۖ إِنَّآ إِلَىٰ رَبِّنَا مُنقَلِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "51",
        "Body": "إِنَّا نَطْمَعُ أَن يَغْفِرَ لَنَا رَبُّنَا خَطَـٰيَـٰنَآ أَن كُنَّآ أَوَّلَ ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "52",
        "Body": "وَأَوْحَيْنَآ إِلَىٰ مُوسَىٰٓ أَنْ أَسْرِ بِعِبَادِىٓ إِنَّكُم مُّتَّبَعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "53",
        "Body": "فَأَرْسَلَ فِرْعَوْنُ فِى ٱلْمَدَآئِنِ حَـٰشِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "54",
        "Body": "إِنَّ هَـٰٓؤُلَآءِ لَشِرْذِمَةٌ قَلِيلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "55",
        "Body": "وَإِنَّهُمْ لَنَا لَغَآئِظُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "56",
        "Body": "وَإِنَّا لَجَمِيعٌ حَـٰذِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "57",
        "Body": "فَأَخْرَجْنَـٰهُم مِّن جَنَّـٰتٍ وَعُيُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "58",
        "Body": "وَكُنُوزٍ وَمَقَامٍ كَرِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "59",
        "Body": "كَذَٰلِكَ وَأَوْرَثْنَـٰهَا بَنِىٓ إِسْرَٰٓءِيلَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "60",
        "Body": "فَأَتْبَعُوهُم مُّشْرِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "61",
        "Body": "فَلَمَّا تَرَٰٓءَا ٱلْجَمْعَانِ قَالَ أَصْحَـٰبُ مُوسَىٰٓ إِنَّا لَمُدْرَكُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "62",
        "Body": "قَالَ كَلَّآ ۖ إِنَّ مَعِىَ رَبِّى سَيَهْدِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "63",
        "Body": "فَأَوْحَيْنَآ إِلَىٰ مُوسَىٰٓ أَنِ ٱضْرِب بِّعَصَاكَ ٱلْبَحْرَ ۖ فَٱنفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَٱلطَّوْدِ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "64",
        "Body": "وَأَزْلَفْنَا ثَمَّ ٱلْـَٔاخَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "65",
        "Body": "وَأَنجَيْنَا مُوسَىٰ وَمَن مَّعَهُۥٓ أَجْمَعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "66",
        "Body": "ثُمَّ أَغْرَقْنَا ٱلْـَٔاخَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "67",
        "Body": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "68",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "69",
        "Body": "وَٱتْلُ عَلَيْهِمْ نَبَأَ إِبْرَٰهِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "70",
        "Body": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَا تَعْبُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "71",
        "Body": "قَالُوا۟ نَعْبُدُ أَصْنَامًا فَنَظَلُّ لَهَا عَـٰكِفِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "72",
        "Body": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "73",
        "Body": "أَوْ يَنفَعُونَكُمْ أَوْ يَضُرُّونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "74",
        "Body": "قَالُوا۟ بَلْ وَجَدْنَآ ءَابَآءَنَا كَذَٰلِكَ يَفْعَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "75",
        "Body": "قَالَ أَفَرَءَيْتُم مَّا كُنتُمْ تَعْبُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "76",
        "Body": "أَنتُمْ وَءَابَآؤُكُمُ ٱلْأَقْدَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "77",
        "Body": "فَإِنَّهُمْ عَدُوٌّ لِّىٓ إِلَّا رَبَّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "78",
        "Body": "ٱلَّذِى خَلَقَنِى فَهُوَ يَهْدِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "79",
        "Body": "وَٱلَّذِى هُوَ يُطْعِمُنِى وَيَسْقِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "80",
        "Body": "وَإِذَا مَرِضْتُ فَهُوَ يَشْفِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "81",
        "Body": "وَٱلَّذِى يُمِيتُنِى ثُمَّ يُحْيِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "82",
        "Body": "وَٱلَّذِىٓ أَطْمَعُ أَن يَغْفِرَ لِى خَطِيٓـَٔتِى يَوْمَ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "83",
        "Body": "رَبِّ هَبْ لِى حُكْمًا وَأَلْحِقْنِى بِٱلصَّـٰلِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "84",
        "Body": "وَٱجْعَل لِّى لِسَانَ صِدْقٍ فِى ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "85",
        "Body": "وَٱجْعَلْنِى مِن وَرَثَةِ جَنَّةِ ٱلنَّعِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "86",
        "Body": "وَٱغْفِرْ لِأَبِىٓ إِنَّهُۥ كَانَ مِنَ ٱلضَّآلِّينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "87",
        "Body": "وَلَا تُخْزِنِى يَوْمَ يُبْعَثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "88",
        "Body": "يَوْمَ لَا يَنفَعُ مَالٌ وَلَا بَنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "89",
        "Body": "إِلَّا مَنْ أَتَى ٱللَّهَ بِقَلْبٍ سَلِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "90",
        "Body": "وَأُزْلِفَتِ ٱلْجَنَّةُ لِلْمُتَّقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "91",
        "Body": "وَبُرِّزَتِ ٱلْجَحِيمُ لِلْغَاوِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "92",
        "Body": "وَقِيلَ لَهُمْ أَيْنَ مَا كُنتُمْ تَعْبُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "93",
        "Body": "مِن دُونِ ٱللَّهِ هَلْ يَنصُرُونَكُمْ أَوْ يَنتَصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "94",
        "Body": "فَكُبْكِبُوا۟ فِيهَا هُمْ وَٱلْغَاوُۥنَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "95",
        "Body": "وَجُنُودُ إِبْلِيسَ أَجْمَعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "96",
        "Body": "قَالُوا۟ وَهُمْ فِيهَا يَخْتَصِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "97",
        "Body": "تَٱللَّهِ إِن كُنَّا لَفِى ضَلَـٰلٍ مُّبِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "98",
        "Body": "إِذْ نُسَوِّيكُم بِرَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "99",
        "Body": "وَمَآ أَضَلَّنَآ إِلَّا ٱلْمُجْرِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "100",
        "Body": "فَمَا لَنَا مِن شَـٰفِعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "101",
        "Body": "وَلَا صَدِيقٍ حَمِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "102",
        "Body": "فَلَوْ أَنَّ لَنَا كَرَّةً فَنَكُونَ مِنَ ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "103",
        "Body": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "104",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "105",
        "Body": "كَذَّبَتْ قَوْمُ نُوحٍ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "106",
        "Body": "إِذْ قَالَ لَهُمْ أَخُوهُمْ نُوحٌ أَلَا تَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "107",
        "Body": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "108",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "109",
        "Body": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "110",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "111",
        "Body": "قَالُوٓا۟ أَنُؤْمِنُ لَكَ وَٱتَّبَعَكَ ٱلْأَرْذَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "112",
        "Body": "قَالَ وَمَا عِلْمِى بِمَا كَانُوا۟ يَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "113",
        "Body": "إِنْ حِسَابُهُمْ إِلَّا عَلَىٰ رَبِّى ۖ لَوْ تَشْعُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "114",
        "Body": "وَمَآ أَنَا۠ بِطَارِدِ ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "115",
        "Body": "إِنْ أَنَا۠ إِلَّا نَذِيرٌ مُّبِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "116",
        "Body": "قَالُوا۟ لَئِن لَّمْ تَنتَهِ يَـٰنُوحُ لَتَكُونَنَّ مِنَ ٱلْمَرْجُومِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "117",
        "Body": "قَالَ رَبِّ إِنَّ قَوْمِى كَذَّبُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "118",
        "Body": "فَٱفْتَحْ بَيْنِى وَبَيْنَهُمْ فَتْحًا وَنَجِّنِى وَمَن مَّعِىَ مِنَ ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "119",
        "Body": "فَأَنجَيْنَـٰهُ وَمَن مَّعَهُۥ فِى ٱلْفُلْكِ ٱلْمَشْحُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "120",
        "Body": "ثُمَّ أَغْرَقْنَا بَعْدُ ٱلْبَاقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "121",
        "Body": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "122",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "123",
        "Body": "كَذَّبَتْ عَادٌ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "124",
        "Body": "إِذْ قَالَ لَهُمْ أَخُوهُمْ هُودٌ أَلَا تَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "125",
        "Body": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "126",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "127",
        "Body": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "128",
        "Body": "أَتَبْنُونَ بِكُلِّ رِيعٍ ءَايَةً تَعْبَثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "129",
        "Body": "وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمْ تَخْلُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "130",
        "Body": "وَإِذَا بَطَشْتُم بَطَشْتُمْ جَبَّارِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "131",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "132",
        "Body": "وَٱتَّقُوا۟ ٱلَّذِىٓ أَمَدَّكُم بِمَا تَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "133",
        "Body": "أَمَدَّكُم بِأَنْعَـٰمٍ وَبَنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "134",
        "Body": "وَجَنَّـٰتٍ وَعُيُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "135",
        "Body": "إِنِّىٓ أَخَافُ عَلَيْكُمْ عَذَابَ يَوْمٍ عَظِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "136",
        "Body": "قَالُوا۟ سَوَآءٌ عَلَيْنَآ أَوَعَظْتَ أَمْ لَمْ تَكُن مِّنَ ٱلْوَٰعِظِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "137",
        "Body": "إِنْ هَـٰذَآ إِلَّا خُلُقُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "138",
        "Body": "وَمَا نَحْنُ بِمُعَذَّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "139",
        "Body": "فَكَذَّبُوهُ فَأَهْلَكْنَـٰهُمْ ۗ إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "140",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "141",
        "Body": "كَذَّبَتْ ثَمُودُ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "142",
        "Body": "إِذْ قَالَ لَهُمْ أَخُوهُمْ صَـٰلِحٌ أَلَا تَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "143",
        "Body": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "144",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "145",
        "Body": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "146",
        "Body": "أَتُتْرَكُونَ فِى مَا هَـٰهُنَآ ءَامِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "147",
        "Body": "فِى جَنَّـٰتٍ وَعُيُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "148",
        "Body": "وَزُرُوعٍ وَنَخْلٍ طَلْعُهَا هَضِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "149",
        "Body": "وَتَنْحِتُونَ مِنَ ٱلْجِبَالِ بُيُوتًا فَـٰرِهِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "150",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "151",
        "Body": "وَلَا تُطِيعُوٓا۟ أَمْرَ ٱلْمُسْرِفِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "152",
        "Body": "ٱلَّذِينَ يُفْسِدُونَ فِى ٱلْأَرْضِ وَلَا يُصْلِحُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "153",
        "Body": "قَالُوٓا۟ إِنَّمَآ أَنتَ مِنَ ٱلْمُسَحَّرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "154",
        "Body": "مَآ أَنتَ إِلَّا بَشَرٌ مِّثْلُنَا فَأْتِ بِـَٔايَةٍ إِن كُنتَ مِنَ ٱلصَّـٰدِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "155",
        "Body": "قَالَ هَـٰذِهِۦ نَاقَةٌ لَّهَا شِرْبٌ وَلَكُمْ شِرْبُ يَوْمٍ مَّعْلُومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "156",
        "Body": "وَلَا تَمَسُّوهَا بِسُوٓءٍ فَيَأْخُذَكُمْ عَذَابُ يَوْمٍ عَظِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "157",
        "Body": "فَعَقَرُوهَا فَأَصْبَحُوا۟ نَـٰدِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "158",
        "Body": "فَأَخَذَهُمُ ٱلْعَذَابُ ۗ إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "159",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "160",
        "Body": "كَذَّبَتْ قَوْمُ لُوطٍ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "161",
        "Body": "إِذْ قَالَ لَهُمْ أَخُوهُمْ لُوطٌ أَلَا تَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "162",
        "Body": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "163",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "164",
        "Body": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "165",
        "Body": "أَتَأْتُونَ ٱلذُّكْرَانَ مِنَ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "166",
        "Body": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُم مِّنْ أَزْوَٰجِكُم ۚ بَلْ أَنتُمْ قَوْمٌ عَادُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "167",
        "Body": "قَالُوا۟ لَئِن لَّمْ تَنتَهِ يَـٰلُوطُ لَتَكُونَنَّ مِنَ ٱلْمُخْرَجِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "168",
        "Body": "قَالَ إِنِّى لِعَمَلِكُم مِّنَ ٱلْقَالِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "169",
        "Body": "رَبِّ نَجِّنِى وَأَهْلِى مِمَّا يَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "170",
        "Body": "فَنَجَّيْنَـٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "171",
        "Body": "إِلَّا عَجُوزًا فِى ٱلْغَـٰبِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "172",
        "Body": "ثُمَّ دَمَّرْنَا ٱلْـَٔاخَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "173",
        "Body": "وَأَمْطَرْنَا عَلَيْهِم مَّطَرًا ۖ فَسَآءَ مَطَرُ ٱلْمُنذَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "174",
        "Body": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "175",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "176",
        "Body": "كَذَّبَ أَصْحَـٰبُ لْـَٔيْكَةِ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "177",
        "Body": "إِذْ قَالَ لَهُمْ شُعَيْبٌ أَلَا تَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "178",
        "Body": "إِنِّى لَكُمْ رَسُولٌ أَمِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "179",
        "Body": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "180",
        "Body": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "181",
        "Body": "أَوْفُوا۟ ٱلْكَيْلَ وَلَا تَكُونُوا۟ مِنَ ٱلْمُخْسِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "182",
        "Body": "وَزِنُوا۟ بِٱلْقِسْطَاسِ ٱلْمُسْتَقِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "183",
        "Body": "وَلَا تَبْخَسُوا۟ ٱلنَّاسَ أَشْيَآءَهُمْ وَلَا تَعْثَوْا۟ فِى ٱلْأَرْضِ مُفْسِدِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "184",
        "Body": "وَٱتَّقُوا۟ ٱلَّذِى خَلَقَكُمْ وَٱلْجِبِلَّةَ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "185",
        "Body": "قَالُوٓا۟ إِنَّمَآ أَنتَ مِنَ ٱلْمُسَحَّرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "186",
        "Body": "وَمَآ أَنتَ إِلَّا بَشَرٌ مِّثْلُنَا وَإِن نَّظُنُّكَ لَمِنَ ٱلْكَـٰذِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "187",
        "Body": "فَأَسْقِطْ عَلَيْنَا كِسَفًا مِّنَ ٱلسَّمَآءِ إِن كُنتَ مِنَ ٱلصَّـٰدِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "188",
        "Body": "قَالَ رَبِّىٓ أَعْلَمُ بِمَا تَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "189",
        "Body": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ ٱلظُّلَّةِ ۚ إِنَّهُۥ كَانَ عَذَابَ يَوْمٍ عَظِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "190",
        "Body": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "191",
        "Body": "وَإِنَّ رَبَّكَ لَهُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "192",
        "Body": "وَإِنَّهُۥ لَتَنزِيلُ رَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "193",
        "Body": "نَزَلَ بِهِ ٱلرُّوحُ ٱلْأَمِينُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "194",
        "Body": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ ٱلْمُنذِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "195",
        "Body": "بِلِسَانٍ عَرَبِىٍّ مُّبِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "196",
        "Body": "وَإِنَّهُۥ لَفِى زُبُرِ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "197",
        "Body": "أَوَلَمْ يَكُن لَّهُمْ ءَايَةً أَن يَعْلَمَهُۥ عُلَمَـٰٓؤُا۟ بَنِىٓ إِسْرَٰٓءِيلَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "198",
        "Body": "وَلَوْ نَزَّلْنَـٰهُ عَلَىٰ بَعْضِ ٱلْأَعْجَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "199",
        "Body": "فَقَرَأَهُۥ عَلَيْهِم مَّا كَانُوا۟ بِهِۦ مُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "200",
        "Body": "كَذَٰلِكَ سَلَكْنَـٰهُ فِى قُلُوبِ ٱلْمُجْرِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "201",
        "Body": "لَا يُؤْمِنُونَ بِهِۦ حَتَّىٰ يَرَوُا۟ ٱلْعَذَابَ ٱلْأَلِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "202",
        "Body": "فَيَأْتِيَهُم بَغْتَةً وَهُمْ لَا يَشْعُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "203",
        "Body": "فَيَقُولُوا۟ هَلْ نَحْنُ مُنظَرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "204",
        "Body": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "205",
        "Body": "أَفَرَءَيْتَ إِن مَّتَّعْنَـٰهُمْ سِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "206",
        "Body": "ثُمَّ جَآءَهُم مَّا كَانُوا۟ يُوعَدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "207",
        "Body": "مَآ أَغْنَىٰ عَنْهُم مَّا كَانُوا۟ يُمَتَّعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "208",
        "Body": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا لَهَا مُنذِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "209",
        "Body": "ذِكْرَىٰ وَمَا كُنَّا ظَـٰلِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "210",
        "Body": "وَمَا تَنَزَّلَتْ بِهِ ٱلشَّيَـٰطِينُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "211",
        "Body": "وَمَا يَنۢبَغِى لَهُمْ وَمَا يَسْتَطِيعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "212",
        "Body": "إِنَّهُمْ عَنِ ٱلسَّمْعِ لَمَعْزُولُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "213",
        "Body": "فَلَا تَدْعُ مَعَ ٱللَّهِ إِلَـٰهًا ءَاخَرَ فَتَكُونَ مِنَ ٱلْمُعَذَّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "214",
        "Body": "وَأَنذِرْ عَشِيرَتَكَ ٱلْأَقْرَبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "215",
        "Body": "وَٱخْفِضْ جَنَاحَكَ لِمَنِ ٱتَّبَعَكَ مِنَ ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "216",
        "Body": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّى بَرِىٓءٌ مِّمَّا تَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "217",
        "Body": "وَتَوَكَّلْ عَلَى ٱلْعَزِيزِ ٱلرَّحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "218",
        "Body": "ٱلَّذِى يَرَىٰكَ حِينَ تَقُومُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "219",
        "Body": "وَتَقَلُّبَكَ فِى ٱلسَّـٰجِدِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "220",
        "Body": "إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "221",
        "Body": "هَلْ أُنَبِّئُكُمْ عَلَىٰ مَن تَنَزَّلُ ٱلشَّيَـٰطِينُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "222",
        "Body": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "223",
        "Body": "يُلْقُونَ ٱلسَّمْعَ وَأَكْثَرُهُمْ كَـٰذِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "26",
        "AyahNumber": "224",
        "Body": "وَٱلشُّعَرَآءُ يَتَّبِعُهُمُ ٱلْغَاوُۥنَ"
    },
    {
