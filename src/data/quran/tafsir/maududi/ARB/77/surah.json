[
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "1",
        "Body": "وَٱلْمُرْسَلَـٰتِ عُرْفًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "2",
        "Body": "فَٱلْعَـٰصِفَـٰتِ عَصْفًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "3",
        "Body": "وَٱلنَّـٰشِرَٰتِ نَشْرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "4",
        "Body": "فَٱلْفَـٰرِقَـٰتِ فَرْقًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "5",
        "Body": "فَٱلْمُلْقِيَـٰتِ ذِكْرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "6",
        "Body": "عُذْرًا أَوْ نُذْرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "7",
        "Body": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "8",
        "Body": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "9",
        "Body": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "10",
        "Body": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "11",
        "Body": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "12",
        "Body": "لِأَىِّ يَوْمٍ أُجِّلَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "13",
        "Body": "لِيَوْمِ ٱلْفَصْلِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "14",
        "Body": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "15",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "16",
        "Body": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "17",
        "Body": "ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "18",
        "Body": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "19",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "20",
        "Body": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "21",
        "Body": "فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "22",
        "Body": "إِلَىٰ قَدَرٍ مَّعْلُومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "23",
        "Body": "فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "24",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "25",
        "Body": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "26",
        "Body": "أَحْيَآءً وَأَمْوَٰتًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "27",
        "Body": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "28",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "29",
        "Body": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "30",
        "Body": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "31",
        "Body": "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "32",
        "Body": "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "33",
        "Body": "كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "34",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "35",
        "Body": "هَـٰذَا يَوْمُ لَا يَنطِقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "36",
        "Body": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "37",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "38",
        "Body": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "39",
        "Body": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "40",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "41",
        "Body": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "42",
        "Body": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "43",
        "Body": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "44",
        "Body": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "45",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "46",
        "Body": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "47",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "48",
        "Body": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "49",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "77",
        "AyahNumber": "50",
        "Body": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ"
    }
]