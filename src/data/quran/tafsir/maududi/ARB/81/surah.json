[
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "1",
        "Body": "إِذَا ٱلشَّمْسُ كُوِّرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "2",
        "Body": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "3",
        "Body": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "4",
        "Body": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "5",
        "Body": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "6",
        "Body": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "7",
        "Body": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "8",
        "Body": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "9",
        "Body": "بِأَىِّ ذَنۢبٍ قُتِلَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "10",
        "Body": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "11",
        "Body": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "12",
        "Body": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "13",
        "Body": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "14",
        "Body": "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "15",
        "Body": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "16",
        "Body": "ٱلْجَوَارِ ٱلْكُنَّسِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "17",
        "Body": "وَٱلَّيْلِ إِذَا عَسْعَسَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "18",
        "Body": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "19",
        "Body": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "20",
        "Body": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "21",
        "Body": "مُّطَاعٍ ثَمَّ أَمِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "22",
        "Body": "وَمَا صَاحِبُكُم بِمَجْنُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "23",
        "Body": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "24",
        "Body": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "25",
        "Body": "وَمَا هُوَ بِقَوْلِ شَيْطَـٰنٍ رَّجِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "26",
        "Body": "فَأَيْنَ تَذْهَبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "27",
        "Body": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "28",
        "Body": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "81",
        "AyahNumber": "29",
        "Body": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَـٰلَمِينَ"
    }
]