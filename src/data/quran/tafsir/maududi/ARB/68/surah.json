[
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "1",
        "Body": "نٓ ۚ وَٱلْقَلَمِ وَمَا يَسْطُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "2",
        "Body": "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "3",
        "Body": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "4",
        "Body": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "5",
        "Body": "فَسَتُبْصِرُ وَيُبْصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "6",
        "Body": "بِأَييِّكُمُ ٱلْمَفْتُونُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "7",
        "Body": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِٱلْمُهْتَدِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "8",
        "Body": "فَلَا تُطِعِ ٱلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "9",
        "Body": "وَدُّوا۟ لَوْ تُدْهِنُ فَيُدْهِنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "10",
        "Body": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "11",
        "Body": "هَمَّازٍ مَّشَّآءٍۭ بِنَمِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "12",
        "Body": "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "13",
        "Body": "عُتُلٍّۭ بَعْدَ ذَٰلِكَ زَنِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "14",
        "Body": "أَن كَانَ ذَا مَالٍ وَبَنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "15",
        "Body": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَـٰتُنَا قَالَ أَسَـٰطِيرُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "16",
        "Body": "سَنَسِمُهُۥ عَلَى ٱلْخُرْطُومِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "17",
        "Body": "إِنَّا بَلَوْنَـٰهُمْ كَمَا بَلَوْنَآ أَصْحَـٰبَ ٱلْجَنَّةِ إِذْ أَقْسَمُوا۟ لَيَصْرِمُنَّهَا مُصْبِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "18",
        "Body": "وَلَا يَسْتَثْنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "19",
        "Body": "فَطَافَ عَلَيْهَا طَآئِفٌ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "20",
        "Body": "فَأَصْبَحَتْ كَٱلصَّرِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "21",
        "Body": "فَتَنَادَوْا۟ مُصْبِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "22",
        "Body": "أَنِ ٱغْدُوا۟ عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَـٰرِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "23",
        "Body": "فَٱنطَلَقُوا۟ وَهُمْ يَتَخَـٰفَتُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "24",
        "Body": "أَن لَّا يَدْخُلَنَّهَا ٱلْيَوْمَ عَلَيْكُم مِّسْكِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "25",
        "Body": "وَغَدَوْا۟ عَلَىٰ حَرْدٍ قَـٰدِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "26",
        "Body": "فَلَمَّا رَأَوْهَا قَالُوٓا۟ إِنَّا لَضَآلُّونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "27",
        "Body": "بَلْ نَحْنُ مَحْرُومُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "28",
        "Body": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "29",
        "Body": "قَالُوا۟ سُبْحَـٰنَ رَبِّنَآ إِنَّا كُنَّا ظَـٰلِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "30",
        "Body": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَـٰوَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "31",
        "Body": "قَالُوا۟ يَـٰوَيْلَنَآ إِنَّا كُنَّا طَـٰغِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "32",
        "Body": "عَسَىٰ رَبُّنَآ أَن يُبْدِلَنَا خَيْرًا مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "33",
        "Body": "كَذَٰلِكَ ٱلْعَذَابُ ۖ وَلَعَذَابُ ٱلْـَٔاخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا۟ يَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "34",
        "Body": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّـٰتِ ٱلنَّعِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "35",
        "Body": "أَفَنَجْعَلُ ٱلْمُسْلِمِينَ كَٱلْمُجْرِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "36",
        "Body": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "37",
        "Body": "أَمْ لَكُمْ كِتَـٰبٌ فِيهِ تَدْرُسُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "38",
        "Body": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "39",
        "Body": "أَمْ لَكُمْ أَيْمَـٰنٌ عَلَيْنَا بَـٰلِغَةٌ إِلَىٰ يَوْمِ ٱلْقِيَـٰمَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "40",
        "Body": "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "41",
        "Body": "أَمْ لَهُمْ شُرَكَآءُ فَلْيَأْتُوا۟ بِشُرَكَآئِهِمْ إِن كَانُوا۟ صَـٰدِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "42",
        "Body": "يَوْمَ يُكْشَفُ عَن سَاقٍ وَيُدْعَوْنَ إِلَى ٱلسُّجُودِ فَلَا يَسْتَطِيعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "43",
        "Body": "خَـٰشِعَةً أَبْصَـٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا۟ يُدْعَوْنَ إِلَى ٱلسُّجُودِ وَهُمْ سَـٰلِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "44",
        "Body": "فَذَرْنِى وَمَن يُكَذِّبُ بِهَـٰذَا ٱلْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "45",
        "Body": "وَأُمْلِى لَهُمْ ۚ إِنَّ كَيْدِى مَتِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "46",
        "Body": "أَمْ تَسْـَٔلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "47",
        "Body": "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "48",
        "Body": "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "49",
        "Body": "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٌ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلْعَرَآءِ وَهُوَ مَذْمُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "50",
        "Body": "فَٱجْتَبَـٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّـٰلِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "51",
        "Body": "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُوا۟ لَيُزْلِقُونَكَ بِأَبْصَـٰرِهِمْ لَمَّا سَمِعُوا۟ ٱلذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "68",
        "AyahNumber": "52",
        "Body": "وَمَا هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ"
    }
]