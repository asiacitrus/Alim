[
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "1",
        "Body": "وَيْلٌ لِّلْمُطَفِّفِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "2",
        "Body": "ٱلَّذِينَ إِذَا ٱكْتَالُوا۟ عَلَى ٱلنَّاسِ يَسْتَوْفُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "3",
        "Body": "وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "4",
        "Body": "أَلَا يَظُنُّ أُو۟لَـٰٓئِكَ أَنَّهُم مَّبْعُوثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "5",
        "Body": "لِيَوْمٍ عَظِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "6",
        "Body": "يَوْمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "7",
        "Body": "كَلَّآ إِنَّ كِتَـٰبَ ٱلْفُجَّارِ لَفِى سِجِّينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "8",
        "Body": "وَمَآ أَدْرَىٰكَ مَا سِجِّينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "9",
        "Body": "كِتَـٰبٌ مَّرْقُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "10",
        "Body": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "11",
        "Body": "ٱلَّذِينَ يُكَذِّبُونَ بِيَوْمِ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "12",
        "Body": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "13",
        "Body": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَـٰتُنَا قَالَ أَسَـٰطِيرُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "14",
        "Body": "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُوا۟ يَكْسِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "15",
        "Body": "كَلَّآ إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٍ لَّمَحْجُوبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "16",
        "Body": "ثُمَّ إِنَّهُمْ لَصَالُوا۟ ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "17",
        "Body": "ثُمَّ يُقَالُ هَـٰذَا ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "18",
        "Body": "كَلَّآ إِنَّ كِتَـٰبَ ٱلْأَبْرَارِ لَفِى عِلِّيِّينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "19",
        "Body": "وَمَآ أَدْرَىٰكَ مَا عِلِّيُّونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "20",
        "Body": "كِتَـٰبٌ مَّرْقُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "21",
        "Body": "يَشْهَدُهُ ٱلْمُقَرَّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "22",
        "Body": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "23",
        "Body": "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "24",
        "Body": "تَعْرِفُ فِى وُجُوهِهِمْ نَضْرَةَ ٱلنَّعِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "25",
        "Body": "يُسْقَوْنَ مِن رَّحِيقٍ مَّخْتُومٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "26",
        "Body": "خِتَـٰمُهُۥ مِسْكٌ ۚ وَفِى ذَٰلِكَ فَلْيَتَنَافَسِ ٱلْمُتَنَـٰفِسُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "27",
        "Body": "وَمِزَاجُهُۥ مِن تَسْنِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "28",
        "Body": "عَيْنًا يَشْرَبُ بِهَا ٱلْمُقَرَّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "29",
        "Body": "إِنَّ ٱلَّذِينَ أَجْرَمُوا۟ كَانُوا۟ مِنَ ٱلَّذِينَ ءَامَنُوا۟ يَضْحَكُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "30",
        "Body": "وَإِذَا مَرُّوا۟ بِهِمْ يَتَغَامَزُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "31",
        "Body": "وَإِذَا ٱنقَلَبُوٓا۟ إِلَىٰٓ أَهْلِهِمُ ٱنقَلَبُوا۟ فَكِهِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "32",
        "Body": "وَإِذَا رَأَوْهُمْ قَالُوٓا۟ إِنَّ هَـٰٓؤُلَآءِ لَضَآلُّونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "33",
        "Body": "وَمَآ أُرْسِلُوا۟ عَلَيْهِمْ حَـٰفِظِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "34",
        "Body": "فَٱلْيَوْمَ ٱلَّذِينَ ءَامَنُوا۟ مِنَ ٱلْكُفَّارِ يَضْحَكُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "35",
        "Body": "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "83",
        "AyahNumber": "36",
        "Body": "هَلْ ثُوِّبَ ٱلْكُفَّارُ مَا كَانُوا۟ يَفْعَلُونَ"
    }
]