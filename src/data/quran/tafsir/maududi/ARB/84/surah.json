[
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "1",
        "Body": "إِذَا ٱلسَّمَآءُ ٱنشَقَّتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "2",
        "Body": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "3",
        "Body": "وَإِذَا ٱلْأَرْضُ مُدَّتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "4",
        "Body": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "5",
        "Body": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "6",
        "Body": "يَـٰٓأَيُّهَا ٱلْإِنسَـٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَـٰقِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "7",
        "Body": "فَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِيَمِينِهِۦ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "8",
        "Body": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "9",
        "Body": "وَيَنقَلِبُ إِلَىٰٓ أَهْلِهِۦ مَسْرُورًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "10",
        "Body": "وَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ وَرَآءَ ظَهْرِهِۦ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "11",
        "Body": "فَسَوْفَ يَدْعُوا۟ ثُبُورًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "12",
        "Body": "وَيَصْلَىٰ سَعِيرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "13",
        "Body": "إِنَّهُۥ كَانَ فِىٓ أَهْلِهِۦ مَسْرُورًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "14",
        "Body": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "15",
        "Body": "بَلَىٰٓ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "16",
        "Body": "فَلَآ أُقْسِمُ بِٱلشَّفَقِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "17",
        "Body": "وَٱلَّيْلِ وَمَا وَسَقَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "18",
        "Body": "وَٱلْقَمَرِ إِذَا ٱتَّسَقَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "19",
        "Body": "لَتَرْكَبُنَّ طَبَقًا عَن طَبَقٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "20",
        "Body": "فَمَا لَهُمْ لَا يُؤْمِنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "21",
        "Body": "وَإِذَا قُرِئَ عَلَيْهِمُ ٱلْقُرْءَانُ لَا يَسْجُدُونَ ۩"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "22",
        "Body": "بَلِ ٱلَّذِينَ كَفَرُوا۟ يُكَذِّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "23",
        "Body": "وَٱللَّهُ أَعْلَمُ بِمَا يُوعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "24",
        "Body": "فَبَشِّرْهُم بِعَذَابٍ أَلِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "84",
        "AyahNumber": "25",
        "Body": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۭ"
    }
]