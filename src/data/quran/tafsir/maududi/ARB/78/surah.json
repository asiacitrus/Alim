[
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "1",
        "Body": "عَمَّ يَتَسَآءَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "2",
        "Body": "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "3",
        "Body": "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "4",
        "Body": "كَلَّا سَيَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "5",
        "Body": "ثُمَّ كَلَّا سَيَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "6",
        "Body": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَـٰدًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "7",
        "Body": "وَٱلْجِبَالَ أَوْتَادًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "8",
        "Body": "وَخَلَقْنَـٰكُمْ أَزْوَٰجًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "9",
        "Body": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "10",
        "Body": "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "11",
        "Body": "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "12",
        "Body": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "13",
        "Body": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "14",
        "Body": "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءً ثَجَّاجًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "15",
        "Body": "لِّنُخْرِجَ بِهِۦ حَبًّا وَنَبَاتًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "16",
        "Body": "وَجَنَّـٰتٍ أَلْفَافًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "17",
        "Body": "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَـٰتًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "18",
        "Body": "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "19",
        "Body": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "20",
        "Body": "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "21",
        "Body": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "22",
        "Body": "لِّلطَّـٰغِينَ مَـَٔابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "23",
        "Body": "لَّـٰبِثِينَ فِيهَآ أَحْقَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "24",
        "Body": "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "25",
        "Body": "إِلَّا حَمِيمًا وَغَسَّاقًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "26",
        "Body": "جَزَآءً وِفَاقًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "27",
        "Body": "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "28",
        "Body": "وَكَذَّبُوا۟ بِـَٔايَـٰتِنَا كِذَّابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "29",
        "Body": "وَكُلَّ شَىْءٍ أَحْصَيْنَـٰهُ كِتَـٰبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "30",
        "Body": "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "31",
        "Body": "إِنَّ لِلْمُتَّقِينَ مَفَازًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "32",
        "Body": "حَدَآئِقَ وَأَعْنَـٰبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "33",
        "Body": "وَكَوَاعِبَ أَتْرَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "34",
        "Body": "وَكَأْسًا دِهَاقًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "35",
        "Body": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّٰبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "36",
        "Body": "جَزَآءً مِّن رَّبِّكَ عَطَآءً حِسَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "37",
        "Body": "رَّبِّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَـٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "38",
        "Body": "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَـٰٓئِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَـٰنُ وَقَالَ صَوَابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "39",
        "Body": "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "78",
        "AyahNumber": "40",
        "Body": "إِنَّآ أَنذَرْنَـٰكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَـٰلَيْتَنِى كُنتُ تُرَٰبًۢا"
    }
]