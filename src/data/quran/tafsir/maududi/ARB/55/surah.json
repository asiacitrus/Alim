        "Body": "عَلَّمَهُ ٱلْبَيَانَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "5",
        "Body": "ٱلشَّمْسُ وَٱلْقَمَرُ بِحُسْبَانٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "6",
        "Body": "وَٱلنَّجْمُ وَٱلشَّجَرُ يَسْجُدَانِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "7",
        "Body": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلْمِيزَانَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "8",
        "Body": "أَلَّا تَطْغَوْا۟ فِى ٱلْمِيزَانِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "9",
        "Body": "وَأَقِيمُوا۟ ٱلْوَزْنَ بِٱلْقِسْطِ وَلَا تُخْسِرُوا۟ ٱلْمِيزَانَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "10",
        "Body": "وَٱلْأَرْضَ وَضَعَهَا لِلْأَنَامِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "11",
        "Body": "فِيهَا فَـٰكِهَةٌ وَٱلنَّخْلُ ذَاتُ ٱلْأَكْمَامِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "12",
        "Body": "وَٱلْحَبُّ ذُو ٱلْعَصْفِ وَٱلرَّيْحَانُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "13",
        "Body": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "14",
        "Body": "خَلَقَ ٱلْإِنسَـٰنَ مِن صَلْصَـٰلٍ كَٱلْفَخَّارِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "15",
        "Body": "وَخَلَقَ ٱلْجَآنَّ مِن مَّارِجٍ مِّن نَّارٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "16",
        "Body": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "17",
        "Body": "رَبُّ ٱلْمَشْرِقَيْنِ وَرَبُّ ٱلْمَغْرِبَيْنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "18",
        "Body": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "55",
        "AyahNumber": "19",
        "Body": "مَرَجَ ٱلْبَحْرَيْنِ يَلْتَقِيَانِ"
