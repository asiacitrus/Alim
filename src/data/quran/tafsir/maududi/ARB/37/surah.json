[
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "1",
        "Body": "وَٱلصَّـٰٓفَّـٰتِ صَفًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "2",
        "Body": "فَٱلزَّٰجِرَٰتِ زَجْرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "3",
        "Body": "فَٱلتَّـٰلِيَـٰتِ ذِكْرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "4",
        "Body": "إِنَّ إِلَـٰهَكُمْ لَوَٰحِدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "5",
        "Body": "رَّبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ ٱلْمَشَـٰرِقِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "6",
        "Body": "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِزِينَةٍ ٱلْكَوَاكِبِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "7",
        "Body": "وَحِفْظًا مِّن كُلِّ شَيْطَـٰنٍ مَّارِدٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "8",
        "Body": "لَّا يَسَّمَّعُونَ إِلَى ٱلْمَلَإِ ٱلْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "9",
        "Body": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "10",
        "Body": "إِلَّا مَنْ خَطِفَ ٱلْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٌ ثَاقِبٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "11",
        "Body": "فَٱسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَآ ۚ إِنَّا خَلَقْنَـٰهُم مِّن طِينٍ لَّازِبٍۭ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "12",
        "Body": "بَلْ عَجِبْتَ وَيَسْخَرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "13",
        "Body": "وَإِذَا ذُكِّرُوا۟ لَا يَذْكُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "14",
        "Body": "وَإِذَا رَأَوْا۟ ءَايَةً يَسْتَسْخِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "15",
        "Body": "وَقَالُوٓا۟ إِنْ هَـٰذَآ إِلَّا سِحْرٌ مُّبِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "16",
        "Body": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "17",
        "Body": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "18",
        "Body": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "19",
        "Body": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "20",
        "Body": "وَقَالُوا۟ يَـٰوَيْلَنَا هَـٰذَا يَوْمُ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "21",
        "Body": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "22",
        "Body": "ٱحْشُرُوا۟ ٱلَّذِينَ ظَلَمُوا۟ وَأَزْوَٰجَهُمْ وَمَا كَانُوا۟ يَعْبُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "23",
        "Body": "مِن دُونِ ٱللَّهِ فَٱهْدُوهُمْ إِلَىٰ صِرَٰطِ ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "24",
        "Body": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْـُٔولُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "25",
        "Body": "مَا لَكُمْ لَا تَنَاصَرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "26",
        "Body": "بَلْ هُمُ ٱلْيَوْمَ مُسْتَسْلِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "27",
        "Body": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "28",
        "Body": "قَالُوٓا۟ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ ٱلْيَمِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "29",
        "Body": "قَالُوا۟ بَل لَّمْ تَكُونُوا۟ مُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "30",
        "Body": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَـٰنٍۭ ۖ بَلْ كُنتُمْ قَوْمًا طَـٰغِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "31",
        "Body": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ ۖ إِنَّا لَذَآئِقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "32",
        "Body": "فَأَغْوَيْنَـٰكُمْ إِنَّا كُنَّا غَـٰوِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "33",
        "Body": "فَإِنَّهُمْ يَوْمَئِذٍ فِى ٱلْعَذَابِ مُشْتَرِكُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "34",
        "Body": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "35",
        "Body": "إِنَّهُمْ كَانُوٓا۟ إِذَا قِيلَ لَهُمْ لَآ إِلَـٰهَ إِلَّا ٱللَّهُ يَسْتَكْبِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "36",
        "Body": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓا۟ ءَالِهَتِنَا لِشَاعِرٍ مَّجْنُونٍۭ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "37",
        "Body": "بَلْ جَآءَ بِٱلْحَقِّ وَصَدَّقَ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "38",
        "Body": "إِنَّكُمْ لَذَآئِقُوا۟ ٱلْعَذَابِ ٱلْأَلِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "39",
        "Body": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "40",
        "Body": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "41",
        "Body": "أُو۟لَـٰٓئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "42",
        "Body": "فَوَٰكِهُ ۖ وَهُم مُّكْرَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "43",
        "Body": "فِى جَنَّـٰتِ ٱلنَّعِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "44",
        "Body": "عَلَىٰ سُرُرٍ مُّتَقَـٰبِلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "45",
        "Body": "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍۭ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "46",
        "Body": "بَيْضَآءَ لَذَّةٍ لِّلشَّـٰرِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "47",
        "Body": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "48",
        "Body": "وَعِندَهُمْ قَـٰصِرَٰتُ ٱلطَّرْفِ عِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "49",
        "Body": "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "50",
        "Body": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "51",
        "Body": "قَالَ قَآئِلٌ مِّنْهُمْ إِنِّى كَانَ لِى قَرِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "52",
        "Body": "يَقُولُ أَءِنَّكَ لَمِنَ ٱلْمُصَدِّقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "53",
        "Body": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَدِينُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "54",
        "Body": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "55",
        "Body": "فَٱطَّلَعَ فَرَءَاهُ فِى سَوَآءِ ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "56",
        "Body": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرْدِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "57",
        "Body": "وَلَوْلَا نِعْمَةُ رَبِّى لَكُنتُ مِنَ ٱلْمُحْضَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "58",
        "Body": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "59",
        "Body": "إِلَّا مَوْتَتَنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "60",
        "Body": "إِنَّ هَـٰذَا لَهُوَ ٱلْفَوْزُ ٱلْعَظِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "61",
        "Body": "لِمِثْلِ هَـٰذَا فَلْيَعْمَلِ ٱلْعَـٰمِلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "62",
        "Body": "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ ٱلزَّقُّومِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "63",
        "Body": "إِنَّا جَعَلْنَـٰهَا فِتْنَةً لِّلظَّـٰلِمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "64",
        "Body": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِىٓ أَصْلِ ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "65",
        "Body": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّيَـٰطِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "66",
        "Body": "فَإِنَّهُمْ لَـَٔاكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "67",
        "Body": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِّنْ حَمِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "68",
        "Body": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "69",
        "Body": "إِنَّهُمْ أَلْفَوْا۟ ءَابَآءَهُمْ ضَآلِّينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "70",
        "Body": "فَهُمْ عَلَىٰٓ ءَاثَـٰرِهِمْ يُهْرَعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "71",
        "Body": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "72",
        "Body": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "73",
        "Body": "فَٱنظُرْ كَيْفَ كَانَ عَـٰقِبَةُ ٱلْمُنذَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "74",
        "Body": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "75",
        "Body": "وَلَقَدْ نَادَىٰنَا نُوحٌ فَلَنِعْمَ ٱلْمُجِيبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "76",
        "Body": "وَنَجَّيْنَـٰهُ وَأَهْلَهُۥ مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "77",
        "Body": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ ٱلْبَاقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "78",
        "Body": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "79",
        "Body": "سَلَـٰمٌ عَلَىٰ نُوحٍ فِى ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "80",
        "Body": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "81",
        "Body": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "82",
        "Body": "ثُمَّ أَغْرَقْنَا ٱلْـَٔاخَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "83",
        "Body": "وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "84",
        "Body": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبٍ سَلِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "85",
        "Body": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "86",
        "Body": "أَئِفْكًا ءَالِهَةً دُونَ ٱللَّهِ تُرِيدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "87",
        "Body": "فَمَا ظَنُّكُم بِرَبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "88",
        "Body": "فَنَظَرَ نَظْرَةً فِى ٱلنُّجُومِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "89",
        "Body": "فَقَالَ إِنِّى سَقِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "90",
        "Body": "فَتَوَلَّوْا۟ عَنْهُ مُدْبِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "91",
        "Body": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "92",
        "Body": "مَا لَكُمْ لَا تَنطِقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "93",
        "Body": "فَرَاغَ عَلَيْهِمْ ضَرْبًۢا بِٱلْيَمِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "94",
        "Body": "فَأَقْبَلُوٓا۟ إِلَيْهِ يَزِفُّونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "95",
        "Body": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "96",
        "Body": "وَٱللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "97",
        "Body": "قَالُوا۟ ٱبْنُوا۟ لَهُۥ بُنْيَـٰنًا فَأَلْقُوهُ فِى ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "98",
        "Body": "فَأَرَادُوا۟ بِهِۦ كَيْدًا فَجَعَلْنَـٰهُمُ ٱلْأَسْفَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "99",
        "Body": "وَقَالَ إِنِّى ذَاهِبٌ إِلَىٰ رَبِّى سَيَهْدِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "100",
        "Body": "رَبِّ هَبْ لِى مِنَ ٱلصَّـٰلِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "101",
        "Body": "فَبَشَّرْنَـٰهُ بِغُلَـٰمٍ حَلِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "102",
        "Body": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعْىَ قَالَ يَـٰبُنَىَّ إِنِّىٓ أَرَىٰ فِى ٱلْمَنَامِ أَنِّىٓ أَذْبَحُكَ فَٱنظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَـٰٓأَبَتِ ٱفْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِىٓ إِن شَآءَ ٱللَّهُ مِنَ ٱلصَّـٰبِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "103",
        "Body": "فَلَمَّآ أَسْلَمَا وَتَلَّهُۥ لِلْجَبِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "104",
        "Body": "وَنَـٰدَيْنَـٰهُ أَن يَـٰٓإِبْرَٰهِيمُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "105",
        "Body": "قَدْ صَدَّقْتَ ٱلرُّءْيَآ ۚ إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "106",
        "Body": "إِنَّ هَـٰذَا لَهُوَ ٱلْبَلَـٰٓؤُا۟ ٱلْمُبِينُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "107",
        "Body": "وَفَدَيْنَـٰهُ بِذِبْحٍ عَظِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "108",
        "Body": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "109",
        "Body": "سَلَـٰمٌ عَلَىٰٓ إِبْرَٰهِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "110",
        "Body": "كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "111",
        "Body": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "112",
        "Body": "وَبَشَّرْنَـٰهُ بِإِسْحَـٰقَ نَبِيًّا مِّنَ ٱلصَّـٰلِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "113",
        "Body": "وَبَـٰرَكْنَا عَلَيْهِ وَعَلَىٰٓ إِسْحَـٰقَ ۚ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٌ وَظَالِمٌ لِّنَفْسِهِۦ مُبِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "114",
        "Body": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَـٰرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "115",
        "Body": "وَنَجَّيْنَـٰهُمَا وَقَوْمَهُمَا مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "116",
        "Body": "وَنَصَرْنَـٰهُمْ فَكَانُوا۟ هُمُ ٱلْغَـٰلِبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "117",
        "Body": "وَءَاتَيْنَـٰهُمَا ٱلْكِتَـٰبَ ٱلْمُسْتَبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "118",
        "Body": "وَهَدَيْنَـٰهُمَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "119",
        "Body": "وَتَرَكْنَا عَلَيْهِمَا فِى ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "120",
        "Body": "سَلَـٰمٌ عَلَىٰ مُوسَىٰ وَهَـٰرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "121",
        "Body": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "122",
        "Body": "إِنَّهُمَا مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "123",
        "Body": "وَإِنَّ إِلْيَاسَ لَمِنَ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "124",
        "Body": "إِذْ قَالَ لِقَوْمِهِۦٓ أَلَا تَتَّقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "125",
        "Body": "أَتَدْعُونَ بَعْلًا وَتَذَرُونَ أَحْسَنَ ٱلْخَـٰلِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "126",
        "Body": "ٱللَّهَ رَبَّكُمْ وَرَبَّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "127",
        "Body": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "128",
        "Body": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "129",
        "Body": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "130",
        "Body": "سَلَـٰمٌ عَلَىٰٓ إِلْ يَاسِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "131",
        "Body": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "132",
        "Body": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "133",
        "Body": "وَإِنَّ لُوطًا لَّمِنَ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "134",
        "Body": "إِذْ نَجَّيْنَـٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "135",
        "Body": "إِلَّا عَجُوزًا فِى ٱلْغَـٰبِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "136",
        "Body": "ثُمَّ دَمَّرْنَا ٱلْـَٔاخَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "137",
        "Body": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "138",
        "Body": "وَبِٱلَّيْلِ ۗ أَفَلَا تَعْقِلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "139",
        "Body": "وَإِنَّ يُونُسَ لَمِنَ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "140",
        "Body": "إِذْ أَبَقَ إِلَى ٱلْفُلْكِ ٱلْمَشْحُونِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "141",
        "Body": "فَسَاهَمَ فَكَانَ مِنَ ٱلْمُدْحَضِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "142",
        "Body": "فَٱلْتَقَمَهُ ٱلْحُوتُ وَهُوَ مُلِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "143",
        "Body": "فَلَوْلَآ أَنَّهُۥ كَانَ مِنَ ٱلْمُسَبِّحِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "144",
        "Body": "لَلَبِثَ فِى بَطْنِهِۦٓ إِلَىٰ يَوْمِ يُبْعَثُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "145",
        "Body": "فَنَبَذْنَـٰهُ بِٱلْعَرَآءِ وَهُوَ سَقِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "146",
        "Body": "وَأَنۢبَتْنَا عَلَيْهِ شَجَرَةً مِّن يَقْطِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "147",
        "Body": "وَأَرْسَلْنَـٰهُ إِلَىٰ مِا۟ئَةِ أَلْفٍ أَوْ يَزِيدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "148",
        "Body": "فَـَٔامَنُوا۟ فَمَتَّعْنَـٰهُمْ إِلَىٰ حِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "149",
        "Body": "فَٱسْتَفْتِهِمْ أَلِرَبِّكَ ٱلْبَنَاتُ وَلَهُمُ ٱلْبَنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "150",
        "Body": "أَمْ خَلَقْنَا ٱلْمَلَـٰٓئِكَةَ إِنَـٰثًا وَهُمْ شَـٰهِدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "151",
        "Body": "أَلَآ إِنَّهُم مِّنْ إِفْكِهِمْ لَيَقُولُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "152",
        "Body": "وَلَدَ ٱللَّهُ وَإِنَّهُمْ لَكَـٰذِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "153",
        "Body": "أَصْطَفَى ٱلْبَنَاتِ عَلَى ٱلْبَنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "154",
        "Body": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "155",
        "Body": "أَفَلَا تَذَكَّرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "156",
        "Body": "أَمْ لَكُمْ سُلْطَـٰنٌ مُّبِينٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "157",
        "Body": "فَأْتُوا۟ بِكِتَـٰبِكُمْ إِن كُنتُمْ صَـٰدِقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "158",
        "Body": "وَجَعَلُوا۟ بَيْنَهُۥ وَبَيْنَ ٱلْجِنَّةِ نَسَبًا ۚ وَلَقَدْ عَلِمَتِ ٱلْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "159",
        "Body": "سُبْحَـٰنَ ٱللَّهِ عَمَّا يَصِفُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "160",
        "Body": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "161",
        "Body": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "162",
        "Body": "مَآ أَنتُمْ عَلَيْهِ بِفَـٰتِنِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "163",
        "Body": "إِلَّا مَنْ هُوَ صَالِ ٱلْجَحِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "164",
        "Body": "وَمَا مِنَّآ إِلَّا لَهُۥ مَقَامٌ مَّعْلُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "165",
        "Body": "وَإِنَّا لَنَحْنُ ٱلصَّآفُّونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "166",
        "Body": "وَإِنَّا لَنَحْنُ ٱلْمُسَبِّحُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "167",
        "Body": "وَإِن كَانُوا۟ لَيَقُولُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "168",
        "Body": "لَوْ أَنَّ عِندَنَا ذِكْرًا مِّنَ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "169",
        "Body": "لَكُنَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "170",
        "Body": "فَكَفَرُوا۟ بِهِۦ ۖ فَسَوْفَ يَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "171",
        "Body": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "172",
        "Body": "إِنَّهُمْ لَهُمُ ٱلْمَنصُورُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "173",
        "Body": "وَإِنَّ جُندَنَا لَهُمُ ٱلْغَـٰلِبُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "174",
        "Body": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "175",
        "Body": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "176",
        "Body": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "177",
        "Body": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَآءَ صَبَاحُ ٱلْمُنذَرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "178",
        "Body": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "179",
        "Body": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "180",
        "Body": "سُبْحَـٰنَ رَبِّكَ رَبِّ ٱلْعِزَّةِ عَمَّا يَصِفُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "181",
        "Body": "وَسَلَـٰمٌ عَلَى ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "37",
        "AyahNumber": "182",
        "Body": "وَٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَـٰلَمِينَ"
    }
]