[
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "1",
        "Body": "وَٱلسَّمَآءِ وَٱلطَّارِقِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "2",
        "Body": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "3",
        "Body": "ٱلنَّجْمُ ٱلثَّاقِبُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "4",
        "Body": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "5",
        "Body": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ مِمَّ خُلِقَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "6",
        "Body": "خُلِقَ مِن مَّآءٍ دَافِقٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "7",
        "Body": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "8",
        "Body": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "9",
        "Body": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "10",
        "Body": "فَمَا لَهُۥ مِن قُوَّةٍ وَلَا نَاصِرٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "11",
        "Body": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "12",
        "Body": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "13",
        "Body": "إِنَّهُۥ لَقَوْلٌ فَصْلٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "14",
        "Body": "وَمَا هُوَ بِٱلْهَزْلِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "15",
        "Body": "إِنَّهُمْ يَكِيدُونَ كَيْدًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "16",
        "Body": "وَأَكِيدُ كَيْدًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "86",
        "AyahNumber": "17",
        "Body": "فَمَهِّلِ ٱلْكَـٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا"
    }
]