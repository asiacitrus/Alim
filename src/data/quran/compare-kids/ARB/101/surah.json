    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "3",
        "Body": "وَمَآ أَدْرَىٰكَ مَا ٱلْقَارِعَةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "4",
        "Body": "يَوْمَ يَكُونُ ٱلنَّاسُ كَٱلْفَرَاشِ ٱلْمَبْثُوثِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "5",
        "Body": "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ ٱلْمَنفُوشِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "6",
        "Body": "فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "7",
        "Body": "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "8",
        "Body": "وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "9",
        "Body": "فَأُمُّهُۥ هَاوِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "10",
        "Body": "وَمَآ أَدْرَىٰكَ مَا هِيَهْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "101",
        "AyahNumber": "11",
        "Body": "نَارٌ حَامِيَةٌۢ"
    }
