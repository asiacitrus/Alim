[
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "1",
        "Body": "سَأَلَ سَآئِلٌۢ بِعَذَابٍ وَاقِعٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "2",
        "Body": "لِّلْكَـٰفِرِينَ لَيْسَ لَهُۥ دَافِعٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "3",
        "Body": "مِّنَ ٱللَّهِ ذِى ٱلْمَعَارِجِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "4",
        "Body": "تَعْرُجُ ٱلْمَلَـٰٓئِكَةُ وَٱلرُّوحُ إِلَيْهِ فِى يَوْمٍ كَانَ مِقْدَارُهُۥ خَمْسِينَ أَلْفَ سَنَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "5",
        "Body": "فَٱصْبِرْ صَبْرًا جَمِيلًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "6",
        "Body": "إِنَّهُمْ يَرَوْنَهُۥ بَعِيدًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "7",
        "Body": "وَنَرَىٰهُ قَرِيبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "8",
        "Body": "يَوْمَ تَكُونُ ٱلسَّمَآءُ كَٱلْمُهْلِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "9",
        "Body": "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "10",
        "Body": "وَلَا يَسْـَٔلُ حَمِيمٌ حَمِيمًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "11",
        "Body": "يُبَصَّرُونَهُمْ ۚ يَوَدُّ ٱلْمُجْرِمُ لَوْ يَفْتَدِى مِنْ عَذَابِ يَوْمِئِذٍۭ بِبَنِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "12",
        "Body": "وَصَـٰحِبَتِهِۦ وَأَخِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "13",
        "Body": "وَفَصِيلَتِهِ ٱلَّتِى تُـْٔوِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "14",
        "Body": "وَمَن فِى ٱلْأَرْضِ جَمِيعًا ثُمَّ يُنجِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "15",
        "Body": "كَلَّآ ۖ إِنَّهَا لَظَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "16",
        "Body": "نَزَّاعَةً لِّلشَّوَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "17",
        "Body": "تَدْعُوا۟ مَنْ أَدْبَرَ وَتَوَلَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "18",
        "Body": "وَجَمَعَ فَأَوْعَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "19",
        "Body": "إِنَّ ٱلْإِنسَـٰنَ خُلِقَ هَلُوعًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "20",
        "Body": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "21",
        "Body": "وَإِذَا مَسَّهُ ٱلْخَيْرُ مَنُوعًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "22",
        "Body": "إِلَّا ٱلْمُصَلِّينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "23",
        "Body": "ٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَآئِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "24",
        "Body": "وَٱلَّذِينَ فِىٓ أَمْوَٰلِهِمْ حَقٌّ مَّعْلُومٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "25",
        "Body": "لِّلسَّآئِلِ وَٱلْمَحْرُومِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "26",
        "Body": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوْمِ ٱلدِّينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "27",
        "Body": "وَٱلَّذِينَ هُم مِّنْ عَذَابِ رَبِّهِم مُّشْفِقُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "28",
        "Body": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "29",
        "Body": "وَٱلَّذِينَ هُمْ لِفُرُوجِهِمْ حَـٰفِظُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "30",
        "Body": "إِلَّا عَلَىٰٓ أَزْوَٰجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَـٰنُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "31",
        "Body": "فَمَنِ ٱبْتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُو۟لَـٰٓئِكَ هُمُ ٱلْعَادُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "32",
        "Body": "وَٱلَّذِينَ هُمْ لِأَمَـٰنَـٰتِهِمْ وَعَهْدِهِمْ رَٰعُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "33",
        "Body": "وَٱلَّذِينَ هُم بِشَهَـٰدَٰتِهِمْ قَآئِمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "34",
        "Body": "وَٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "35",
        "Body": "أُو۟لَـٰٓئِكَ فِى جَنَّـٰتٍ مُّكْرَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "36",
        "Body": "فَمَالِ ٱلَّذِينَ كَفَرُوا۟ قِبَلَكَ مُهْطِعِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "37",
        "Body": "عَنِ ٱلْيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "38",
        "Body": "أَيَطْمَعُ كُلُّ ٱمْرِئٍ مِّنْهُمْ أَن يُدْخَلَ جَنَّةَ نَعِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "39",
        "Body": "كَلَّآ ۖ إِنَّا خَلَقْنَـٰهُم مِّمَّا يَعْلَمُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "40",
        "Body": "فَلَآ أُقْسِمُ بِرَبِّ ٱلْمَشَـٰرِقِ وَٱلْمَغَـٰرِبِ إِنَّا لَقَـٰدِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "41",
        "Body": "عَلَىٰٓ أَن نُّبَدِّلَ خَيْرًا مِّنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "42",
        "Body": "فَذَرْهُمْ يَخُوضُوا۟ وَيَلْعَبُوا۟ حَتَّىٰ يُلَـٰقُوا۟ يَوْمَهُمُ ٱلَّذِى يُوعَدُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "43",
        "Body": "يَوْمَ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ سِرَاعًا كَأَنَّهُمْ إِلَىٰ نُصُبٍ يُوفِضُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "70",
        "AyahNumber": "44",
        "Body": "خَـٰشِعَةً أَبْصَـٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۚ ذَٰلِكَ ٱلْيَوْمُ ٱلَّذِى كَانُوا۟ يُوعَدُونَ"
    }
]