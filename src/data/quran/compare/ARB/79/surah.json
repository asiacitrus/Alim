[
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "1",
        "Body": "وَٱلنَّـٰزِعَـٰتِ غَرْقًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "2",
        "Body": "وَٱلنَّـٰشِطَـٰتِ نَشْطًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "3",
        "Body": "وَٱلسَّـٰبِحَـٰتِ سَبْحًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "4",
        "Body": "فَٱلسَّـٰبِقَـٰتِ سَبْقًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "5",
        "Body": "فَٱلْمُدَبِّرَٰتِ أَمْرًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "6",
        "Body": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "7",
        "Body": "تَتْبَعُهَا ٱلرَّادِفَةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "8",
        "Body": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "9",
        "Body": "أَبْصَـٰرُهَا خَـٰشِعَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "10",
        "Body": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "11",
        "Body": "أَءِذَا كُنَّا عِظَـٰمًا نَّخِرَةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "12",
        "Body": "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "13",
        "Body": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "14",
        "Body": "فَإِذَا هُم بِٱلسَّاهِرَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "15",
        "Body": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "16",
        "Body": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "17",
        "Body": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "18",
        "Body": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "19",
        "Body": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "20",
        "Body": "فَأَرَىٰهُ ٱلْـَٔايَةَ ٱلْكُبْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "21",
        "Body": "فَكَذَّبَ وَعَصَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "22",
        "Body": "ثُمَّ أَدْبَرَ يَسْعَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "23",
        "Body": "فَحَشَرَ فَنَادَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "24",
        "Body": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "25",
        "Body": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْـَٔاخِرَةِ وَٱلْأُولَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "26",
        "Body": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "27",
        "Body": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "28",
        "Body": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "29",
        "Body": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "30",
        "Body": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "31",
        "Body": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "32",
        "Body": "وَٱلْجِبَالَ أَرْسَىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "33",
        "Body": "مَتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "34",
        "Body": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "35",
        "Body": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَـٰنُ مَا سَعَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "36",
        "Body": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "37",
        "Body": "فَأَمَّا مَن طَغَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "38",
        "Body": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "39",
        "Body": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "40",
        "Body": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "41",
        "Body": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "42",
        "Body": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "43",
        "Body": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "44",
        "Body": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "45",
        "Body": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "79",
        "AyahNumber": "46",
        "Body": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا"
    }
]