[
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "1",
        "Body": "وَٱلْفَجْرِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "2",
        "Body": "وَلَيَالٍ عَشْرٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "3",
        "Body": "وَٱلشَّفْعِ وَٱلْوَتْرِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "4",
        "Body": "وَٱلَّيْلِ إِذَا يَسْرِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "5",
        "Body": "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "6",
        "Body": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "7",
        "Body": "إِرَمَ ذَاتِ ٱلْعِمَادِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "8",
        "Body": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَـٰدِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "9",
        "Body": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "10",
        "Body": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "11",
        "Body": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَـٰدِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "12",
        "Body": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "13",
        "Body": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "14",
        "Body": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "15",
        "Body": "فَأَمَّا ٱلْإِنسَـٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "16",
        "Body": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَـٰنَنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "17",
        "Body": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "18",
        "Body": "وَلَا تَحَـٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "19",
        "Body": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "20",
        "Body": "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "21",
        "Body": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "22",
        "Body": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "23",
        "Body": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَـٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "24",
        "Body": "يَقُولُ يَـٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "25",
        "Body": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "26",
        "Body": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "27",
        "Body": "يَـٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "28",
        "Body": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "29",
        "Body": "فَٱدْخُلِى فِى عِبَـٰدِى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "89",
        "AyahNumber": "30",
        "Body": "وَٱدْخُلِى جَنَّتِى"
    }
]