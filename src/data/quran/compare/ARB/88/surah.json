[
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "1",
        "Body": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَـٰشِيَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "2",
        "Body": "وُجُوهٌ يَوْمَئِذٍ خَـٰشِعَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "3",
        "Body": "عَامِلَةٌ نَّاصِبَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "4",
        "Body": "تَصْلَىٰ نَارًا حَامِيَةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "5",
        "Body": "تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "6",
        "Body": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "7",
        "Body": "لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "8",
        "Body": "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "9",
        "Body": "لِّسَعْيِهَا رَاضِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "10",
        "Body": "فِى جَنَّةٍ عَالِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "11",
        "Body": "لَّا تَسْمَعُ فِيهَا لَـٰغِيَةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "12",
        "Body": "فِيهَا عَيْنٌ جَارِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "13",
        "Body": "فِيهَا سُرُرٌ مَّرْفُوعَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "14",
        "Body": "وَأَكْوَابٌ مَّوْضُوعَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "15",
        "Body": "وَنَمَارِقُ مَصْفُوفَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "16",
        "Body": "وَزَرَابِىُّ مَبْثُوثَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "17",
        "Body": "أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "18",
        "Body": "وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "19",
        "Body": "وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "20",
        "Body": "وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "21",
        "Body": "فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "22",
        "Body": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "23",
        "Body": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "24",
        "Body": "فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "25",
        "Body": "إِنَّ إِلَيْنَآ إِيَابَهُمْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "88",
        "AyahNumber": "26",
        "Body": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم"
    }
]