[
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "1",
        "Body": "سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "2",
        "Body": "ٱلَّذِى خَلَقَ فَسَوَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "3",
        "Body": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "4",
        "Body": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "5",
        "Body": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "6",
        "Body": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "7",
        "Body": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "8",
        "Body": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "9",
        "Body": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "10",
        "Body": "سَيَذَّكَّرُ مَن يَخْشَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "11",
        "Body": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "12",
        "Body": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "13",
        "Body": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "14",
        "Body": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "15",
        "Body": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "16",
        "Body": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "17",
        "Body": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "18",
        "Body": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "87",
        "AyahNumber": "19",
        "Body": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ"
    }
]