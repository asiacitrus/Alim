[
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "1",
        "Body": "لَآ أُقْسِمُ بِهَـٰذَا ٱلْبَلَدِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "2",
        "Body": "وَأَنتَ حِلٌّۢ بِهَـٰذَا ٱلْبَلَدِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "3",
        "Body": "وَوَالِدٍ وَمَا وَلَدَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "4",
        "Body": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِى كَبَدٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "5",
        "Body": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "6",
        "Body": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "7",
        "Body": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "8",
        "Body": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "9",
        "Body": "وَلِسَانًا وَشَفَتَيْنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "10",
        "Body": "وَهَدَيْنَـٰهُ ٱلنَّجْدَيْنِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "11",
        "Body": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "12",
        "Body": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "13",
        "Body": "فَكُّ رَقَبَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "14",
        "Body": "أَوْ إِطْعَـٰمٌ فِى يَوْمٍ ذِى مَسْغَبَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "15",
        "Body": "يَتِيمًا ذَا مَقْرَبَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "16",
        "Body": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "17",
        "Body": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "18",
        "Body": "أُو۟لَـٰٓئِكَ أَصْحَـٰبُ ٱلْمَيْمَنَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "19",
        "Body": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَـٰتِنَا هُمْ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "90",
        "AyahNumber": "20",
        "Body": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌۢ"
    }
]