[
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "1",
        "Body": "ٱلْحَآقَّةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "2",
        "Body": "مَا ٱلْحَآقَّةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "3",
        "Body": "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "4",
        "Body": "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "5",
        "Body": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "6",
        "Body": "وَأَمَّا عَادٌ فَأُهْلِكُوا۟ بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "7",
        "Body": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَـٰنِيَةَ أَيَّامٍ حُسُومًا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "8",
        "Body": "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "9",
        "Body": "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَـٰتُ بِٱلْخَاطِئَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "10",
        "Body": "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "11",
        "Body": "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَـٰكُمْ فِى ٱلْجَارِيَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "12",
        "Body": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَآ أُذُنٌ وَٰعِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "13",
        "Body": "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌ وَٰحِدَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "14",
        "Body": "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةً وَٰحِدَةً"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "15",
        "Body": "فَيَوْمَئِذٍ وَقَعَتِ ٱلْوَاقِعَةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "16",
        "Body": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍ وَاهِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "17",
        "Body": "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَـٰنِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "18",
        "Body": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "19",
        "Body": "فَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَـٰبِيَهْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "20",
        "Body": "إِنِّى ظَنَنتُ أَنِّى مُلَـٰقٍ حِسَابِيَهْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "21",
        "Body": "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "22",
        "Body": "فِى جَنَّةٍ عَالِيَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "23",
        "Body": "قُطُوفُهَا دَانِيَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "24",
        "Body": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "25",
        "Body": "وَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَـٰلَيْتَنِى لَمْ أُوتَ كِتَـٰبِيَهْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "26",
        "Body": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "27",
        "Body": "يَـٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "28",
        "Body": "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "29",
        "Body": "هَلَكَ عَنِّى سُلْطَـٰنِيَهْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "30",
        "Body": "خُذُوهُ فَغُلُّوهُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "31",
        "Body": "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "32",
        "Body": "ثُمَّ فِى سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَٱسْلُكُوهُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "33",
        "Body": "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "34",
        "Body": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "35",
        "Body": "فَلَيْسَ لَهُ ٱلْيَوْمَ هَـٰهُنَا حَمِيمٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "36",
        "Body": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "37",
        "Body": "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَـٰطِـُٔونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "38",
        "Body": "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "39",
        "Body": "وَمَا لَا تُبْصِرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "40",
        "Body": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "41",
        "Body": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "42",
        "Body": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "43",
        "Body": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "44",
        "Body": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "45",
        "Body": "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "46",
        "Body": "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "47",
        "Body": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَـٰجِزِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "48",
        "Body": "وَإِنَّهُۥ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "49",
        "Body": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "50",
        "Body": "وَإِنَّهُۥ لَحَسْرَةٌ عَلَى ٱلْكَـٰفِرِينَ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "51",
        "Body": "وَإِنَّهُۥ لَحَقُّ ٱلْيَقِينِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "69",
        "AyahNumber": "52",
        "Body": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ"
    }
]