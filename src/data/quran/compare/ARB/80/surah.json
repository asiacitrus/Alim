[
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "1",
        "Body": "عَبَسَ وَتَوَلَّىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "2",
        "Body": "أَن جَآءَهُ ٱلْأَعْمَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "3",
        "Body": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "4",
        "Body": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "5",
        "Body": "أَمَّا مَنِ ٱسْتَغْنَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "6",
        "Body": "فَأَنتَ لَهُۥ تَصَدَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "7",
        "Body": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "8",
        "Body": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "9",
        "Body": "وَهُوَ يَخْشَىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "10",
        "Body": "فَأَنتَ عَنْهُ تَلَهَّىٰ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "11",
        "Body": "كَلَّآ إِنَّهَا تَذْكِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "12",
        "Body": "فَمَن شَآءَ ذَكَرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "13",
        "Body": "فِى صُحُفٍ مُّكَرَّمَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "14",
        "Body": "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "15",
        "Body": "بِأَيْدِى سَفَرَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "16",
        "Body": "كِرَامٍۭ بَرَرَةٍ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "17",
        "Body": "قُتِلَ ٱلْإِنسَـٰنُ مَآ أَكْفَرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "18",
        "Body": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "19",
        "Body": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "20",
        "Body": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "21",
        "Body": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "22",
        "Body": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "23",
        "Body": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "24",
        "Body": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ إِلَىٰ طَعَامِهِۦٓ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "25",
        "Body": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "26",
        "Body": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "27",
        "Body": "فَأَنۢبَتْنَا فِيهَا حَبًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "28",
        "Body": "وَعِنَبًا وَقَضْبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "29",
        "Body": "وَزَيْتُونًا وَنَخْلًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "30",
        "Body": "وَحَدَآئِقَ غُلْبًا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "31",
        "Body": "وَفَـٰكِهَةً وَأَبًّا"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "32",
        "Body": "مَّتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "33",
        "Body": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "34",
        "Body": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "35",
        "Body": "وَأُمِّهِۦ وَأَبِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "36",
        "Body": "وَصَـٰحِبَتِهِۦ وَبَنِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "37",
        "Body": "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "38",
        "Body": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "39",
        "Body": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "40",
        "Body": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "41",
        "Body": "تَرْهَقُهَا قَتَرَةٌ"
    },
    {
        "BookCode": "ARB",
        "SurahNumber": "80",
        "AyahNumber": "42",
        "Body": "أُو۟لَـٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ"
    }
]