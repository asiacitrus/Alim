[
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/088001",
        "Body":  "Hath there come unto thee tidings of the Overwhelming?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/088010",
        "Body":  "In a high garden"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/088011",
        "Body":  "Where they hear no idle speech,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/088012",
        "Body":  "Wherein is a gushing spring,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/088013",
        "Body":  "Wherein are couches raised"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/088014",
        "Body":  "And goblets set at hand"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/088015",
        "Body":  "And cushions ranged"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/088016",
        "Body":  "And silken carts spread"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/088017",
        "Body":  "Will they not regard the camels, how they are created?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/088018",
        "Body":  "And the heaven, how it is railed?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/088019",
        "Body":  "And the hills, how they are set up?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/088002",
        "Body":  "On that day (many) faces will be downcast,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/088020",
        "Body":  "And the earth, how it is spread?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/088021",
        "Body":  "Remind them, for thou art but a remembrancer,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/088022",
        "Body":  "Thou art not at all a warder over them."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/088023",
        "Body":  "But whoso is averse and disbelieveth,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/088024",
        "Body":  "Allah will punish him with direst punishment."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/088025",
        "Body":  "Lo! unto Us is their return"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/088026",
        "Body":  "And Ours their reckoning."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/088003",
        "Body":  "Toiling, weary,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/088004",
        "Body":  "Scorched by burning fire,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/088005",
        "Body":  "Drinking from a boiling spring,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/088006",
        "Body":  "No food for them save bitter thorn fruit"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/088007",
        "Body":  "Which doth not nourish nor release from hunger."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/088008",
        "Body":  "In that day other faces will be calm,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "88",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/088009",
        "Body":  "Glad for their effort past,"
    }
]
