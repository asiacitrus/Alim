    {
        "BookCode":  "PIK",
        "SurahNumber":  "85",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/085005",
        "Body":  "Of the fuel-fed fire,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "85",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/085006",
        "Body":  "When they sat by it,"
