[
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/096001",
        "Body":  "Read: In the name of thy Lord who createth,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/096010",
        "Body":  "A slave when he prayeth?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/096011",
        "Body":  "Hast thou seen if he (relieth) on the guidance (of Allah)"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/096012",
        "Body":  "Or enjoineth piety?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/096013",
        "Body":  "Hast thou seen if he denieth (Allah\u0027s guidance) and is froward?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/096014",
        "Body":  "Is he then unaware that Allah seeth?"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/096015",
        "Body":  "Nay, but if he cease not, We will seize him by the forelock--"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/096016",
        "Body":  "The lying, sinful forelock--"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/096017",
        "Body":  "Then let him call upon his henchmen!"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/096018",
        "Body":  "We will call the guards of hell."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/096019",
        "Body":  "Nay! Obey not thou him. But prostrate thyself, and draw near\n\t\t\t(unto Allah)."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/096002",
        "Body":  "Createth man from a clot."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/096003",
        "Body":  "Read: And thy Lord is the Most Bounteous,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/096004",
        "Body":  "Who teacheth by the pen,"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/096005",
        "Body":  "Teacheth man that which be knew not."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/096006",
        "Body":  "Nay, but verily man is rebellious"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/096007",
        "Body":  "That he thinketh himself independent!"
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/096008",
        "Body":  "Lo! unto thy Lord is the return."
    },
    {
        "BookCode":  "PIK",
        "SurahNumber":  "96",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/096009",
        "Body":  "Hast thou seen him who dissuadeth"
    }
]
